{
  "name": "@l-v-yonsama/multi-platform-database-drivers",
  "version": "0.1.163",
  "description": "multi-platform database drivers in TypeScript.",
  "engines": {
    "node": ">= 16"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.1",
    "@types/jest": "~29.2",
    "@types/json-rules-engine": "^4.1.0",
    "@types/node": "~18",
    "@types/pg": "^8.6.6",
    "@types/pluralize": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "~5.44",
    "@typescript-eslint/parser": "~5.44",
    "eslint": "~8.28",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "jest": "~29.3",
    "prettier": "~2.8",
    "rimraf": "~3.0",
    "ts-jest": "~29.0",
    "tsutils": "~3.21",
    "typescript": "~4.9"
  },
  "scripts": {
    "start": "node built/src/index.js",
    "clean": "rimraf coverage built tmp",
    "prebuild": "npm run lint",
    "build": "npm run clean && tsc -p tsconfig.json",
    "build:watch": "npm run clean && tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "prepublishOnly": "npm run build:release",
    "lint": "eslint . --ext .ts --ext .mts",
    "lint:fix": "eslint --fix . --ext .ts --ext .mts",
    "examples:mysql": "npm run build && node built/src/examples/mysql.js",
    "examples:rules": "npm run build && node built/src/examples/rules.js",
    "test0": "jest --coverage",
    "test": "npm run clean && jest --runInBand",
    "test:driver:mysql": "npm run clean && jest __tests__/db/drivers/MySQLDriver.test.ts",
    "test:driver:postgres": "npm run clean && jest __tests__/db/drivers/PostgresDriver.test.ts",
    "test:driver:redis": "npm run clean && jest __tests__/db/drivers/RedisDriver.test.ts",
    "test:driver:sqs": "npm run clean && jest __tests__/db/drivers/AwsSQSDriver.test.ts",
    "test:driver:cw": "npm run clean && jest __tests__/db/drivers/AwsCloudwatchDriver.test.ts",
    "test:driver:minio": "npm run clean && jest __tests__/db/drivers/AwsS3Driver.test.ts",
    "test:helper:sql": "npm run clean && jest __tests__/helpers/SQLHelper.test.ts",
    "test:helper:resource": "npm run clean && jest __tests__/helpers/ResourceHelper.test.ts",
    "test:helper:engine": "npm run clean && jest __tests__/helpers/RuleEngine.test.ts",
    "test:helper:resolver": "npm run clean && jest __tests__/helpers/CodeResolver.test.ts",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "author": "l-v-yonsama <32474032+l-v-yonsama@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.306.0",
    "@aws-sdk/client-s3": "^3.279.0",
    "@aws-sdk/client-sqs": "^3.303.0",
    "@aws-sdk/credential-providers": "^3.279.0",
    "bytes": "^3.1.2",
    "dayjs": "^1.11.7",
    "enum-values": "^1.2.1",
    "get-port-please": "^3.0.1",
    "ioredis": "^5.3.1",
    "jconv": "^0.1.5",
    "json-rules-engine": "^6.1.2",
    "list-it": "^1.3.10",
    "mysql2": "^3.1.2",
    "pg": "^8.9.0",
    "pgsql-ast-parser": "^11.0.1",
    "pluralize": "^8.0.0",
    "pretty-bytes": "^6.1.0",
    "short-unique-id": "^4.4.4",
    "shuffle-array": "^1.0.1",
    "simple-statistics": "^7.8.3",
    "tslib": "~2.4",
    "tunnel-ssh": "^5.0.5"
  },
  "volta": {
    "node": "18.12.1"
  },
  "main": "built/src/index.js",
  "types": "built/src/index.d.ts",
  "files": [
    "built/src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/l-v-yonsama/db-drivers.git"
  },
  "keywords": [
    "database clients"
  ],
  "bugs": {
    "url": "https://github.com/l-v-yonsama/db-drivers/issues"
  },
  "homepage": "https://github.com/l-v-yonsama/db-drivers#readme"
}
